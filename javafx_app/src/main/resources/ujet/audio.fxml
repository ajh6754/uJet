<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ujet.audio"> <!-- CHANGE! -->
   <children>
      <Label text="Filter Audio Sources" />
      <VBox alignment="CENTER" spacing="20.0">
        <CheckBox fx:id="microphoneAudio" text="Microphone Audio" />
        <CheckBox fx:id="chromeAudio" text="Chrome Audio" />
        <CheckBox fx:id="discordAudio" text="Discord Audio" />
        <Button fx:id="save" text="Save" onAction"#saveSettings"/>
      </VBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
